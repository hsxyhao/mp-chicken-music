/* pages/play/play.wxss */

.paly {
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  z-index: 500;
  background: #222;
  overflow: hidden;
}
.mask {
  position: absolute;
  margin-top: 40rpx; 
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: -1;
  opacity: .6;
  -webkit-filter: blur(20px);
  filter: blur(20px);
}

.mask-img {
  width: 100%;
  height: 100%;
}

.content {
  height: 100%;
  display: flex;
  flex-direction: column;
}

.header {
  display: flex;
  position: absolute;
  left: 0rpx;
  top: -100px;
  width: 100%;
  flex-direction: column;
  justify-content: center;
}

.song-name {
  line-height: 40px;
  text-align: center;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  font-size: 18px;
  color: #fff;
}

.song-singer {
  line-height: 20px;
  text-align: center;
  font-size: 14px;
  color: #fff;
}

.body {
  position: absolute;
  width: 100%;
  top: 200rpx;
  bottom: 340rpx;
  white-space: nowrap;
  font-size: 0;
  overflow: hidden;
}

.relative {
  position: relative;
  width: 100%;
  height: 100%;
}

.body-style {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: space-around;
  flex-direction: column;
  align-items: center;
}

.body-style-two {
  z-index: 100;
  left: 100%;
}

.lyric-wrapper {
  height: 100%;
  width: 100%;
  text-align: center;
  overflow: hidden;
}

.lyric-text {
  display: block;
  line-height: 32px;
  color: hsla(0,0%,100%,.5);
  font-size: 14px;
}

.body-img {
  width: 540rpx;
  height: 540rpx;
  box-sizing: border-box;
  border-radius: 50%;
  overflow: hidden;
  border: 10px solid hsla(0,0%,100%,.1);
  transform: matrix(-0.999194, -0.0401477, 0.0401477, -0.999194, 0, 0) matrix(-0.967504, 0.252855, -0.252855, -0.967504, 0, 0);
}

.song-image {
  width: 100%;
  height: 100%;
  border-radius: 50%;
  animation:ratate 20s linear infinite;
}

.footer {
  position: absolute;
  bottom: -150px;
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.song-lyric {
  width: 80%;
  overflow: hidden;
  text-align: center;
  height: 20px;
  line-height: 20px;
  font-size: 14px;
  color: hsla(0,0%,100%,.5);
}



.dot-index {
  display: flex;
  justify-content: center;
}

.dot-index-item {
  margin: 0 8rpx;
  width: 16rpx;
  height: 16rpx;
  border-radius: 50%;
  background: hsla(0,0%,100%,.5);
  transition: all .3s cubic-bezier(0.86,0.18,0.82,1.32);
}

.index-selected {
  width: 40rpx ;
  border-radius: 5px;
  background: hsla(0,0%,100%,.8) ;
}

.progress-box {
  width: 100%;
  position: relative;
}

.edit {
  display: flex;
  margin-top: 20rpx;
  height: 80rpx;
  width: 80%;
  justify-content: space-between;
  align-items: center;
  vertical-align: middle;
}


