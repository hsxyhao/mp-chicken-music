/* pages/components/sdetails/sdetails.wxss */

.details {
  position: fixed;
  top: 0;
  bottom: 0;
  width: 100%;
  z-index: 400;
  overflow: hidden;
  transform: translateX(100%);
  background: rgba(7,17,27,1);
}

.deatils-header {
  width: 100%;
}

.title {
  position: absolute;
  top: 30rpx;
  left: 10%;
  z-index: 400;
  width: 80%;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  text-align: center;
  line-height: 80rpx;
  font-size: 18px;
  color: #fff;
}

.back {
  position: absolute;
  top: 30rpx;
  left: 6px;
  margin: 20rpx;
  width: 44rpx;
  height: 44rpx;
  z-index: 400;
  background-size: cover;
  background-image: url('data:image/png;base64,');
}

.details-img {
  position: relative;
  width: 100%;
  height: 0;
  padding-top: 70%;
  transform-origin: top;
  background-size: cover;
  z-index: 200;
}

.filter {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(7,17,27,.4);
}

.play-wrapper {
    position: absolute;
    bottom: 20px;
    z-index: 50;
    width: 100%;
}

.play-wrapper .text{
  display: inline-block;
  vertical-align: middle;
  font-size: 12px;
}

.play {
  display: flex;
  justify-content: center;
  box-sizing: border-box;
  width: 135px;
  padding: 7px 0;
  margin: 0 auto;
  text-align: center;
  border: 1px solid #ffcd32;
  color: #ffcd32;
  border-radius: 100px;
  font-size: 0;
}

.icon-play {
  height: 32rpx;
  width: 32rpx;
  margin-right: 20rpx;
  background-size: cover;
  background-image: url('data:image/png;base64,')
}

.scroll-box {
  position: absolute;
  height: 100%;
  width: 100%;
  background: #222;
  z-index: 100;
}

.details-item {
  display: flex;
  -ms-flex-align: center;
  align-items: center;
  box-sizing: border-box;
  height: 128rpx;
  font-size: 28rpx;
}

.details-title {
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  color: #fff;
}

.details-singers {
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  margin-top: 4px;
  color: hsla(0,0%,100%,.3);
}
.scroll {
  position: relative;
  height: 100%;
  padding-left: 20px;
}

.icon-top {
  flex: 0 0 25px;
  width: 25px;
  margin-right: 30px;
  text-align: center;
}

.icon-num-img {
  display: inline-block;
  width: 25px;
  height: 24px;
  background-size: 25px 24px;
}

.icon-num-normal {
  color: #ffcd32;
  font-size: 18px;
}
.icon-num-0 {
  background-image: url(data:image/png;base64,)
}

.icon-num-1 {
  background-image: url(data:image/png;base64,)
}

.icon-num-2 {
  background-image: url(data:image/png;base64,)
}