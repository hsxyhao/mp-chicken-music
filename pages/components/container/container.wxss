/* pages/components/container/container.wxss */

.singer-details {
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  top: 0;
  z-index: 200;
}

.container {
  position: absolute;
  width: 400%;
  bottom: 0;
  top: 0;
  display: flex;
}

.wrapper {
  position: relative;
  display: flex;
  flex: 1;
  height: 100%;
}


/* index */

.index {
  flex: 1;
}

.header {
  width: 100%;
  height: 80rpx;
}

.swiper {
  width: 100%;
}
.swiper-item {
  width: 100%;
}

.slide-image {
  width: 100%;
  height: 300rpx;
}

.block-title {
  color: #FFCD32;
  font-size: 30rpx;
  padding: 40rpx 0;
}

.music-item {
  display: flex;
  margin: 40rpx;
  margin-top: 0;
}

.music-img {
  width: 120rpx;
  height: 120rpx;
  margin-right: 40rpx;
  overflow-y: scroll;
}

.music-list {
  text-align: center;
  background-color: #222222;
}

.music-text-desc {
  color: #595959;
  font-size: 30rpx;
  text-align: left;
}

.music-text-singer {
  color: #fff;
  font-size: 30rpx;
  text-align: left;
}

.music-info {
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}

.index::-webkit-scrollbar {
  display:none;
  width: 0;
  height: 0;
  color: transparent;
  background-color: transparent;
}

/* singer */
.singer {
  width: 100%;
}

.fixed-title {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
}

.list-view {
  height: 100%;
}
.right {
  position: absolute;
  display: flex;
  z-index: 100;
  flex-direction: column;
  right: 4rpx;
  width: 20px;
  padding: 20px 0;
  top: 0;
  border-radius: 10px;
  text-align: center;
  background: rgba(0,0,0,.3);
  font-family: Helvetica;
}

.right text {
  color: hsla(0,0%,100%,.5);
  font-size: 12px;
  padding: 3rpx;
}

.text-selected {
  color: #FFCD32 !important;
}

.list-title {
  line-height: 60rpx;
  height: 60rpx;
  padding-left: 20px;
  font-size: 12px;
  color: hsla(0,0%,100%,.5);
  background: #333;
}

.singer-item {
  display: flex;
  padding: 40rpx 60rpx 0;
}

.singer-img-box {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  align-items: center;
  box-sizing:border-box;
  overflow:hidden;
}

.singer-img {
  width: 100%;
  height: 100%;
}

.singer-text {
  margin-left: 20px;
  color: hsla(0,0%,100%,.5);
  font-size: 14px;
  display: flex;
  align-items: center;
}


/* top */
.top {
  width: 100%;
  background-color: #222222;
  padding-bottom: 20rpx;
  overflow-y: scroll;
}

.scroll-box-wrapper {
  width: 90%;
  margin: 0 auto;
}

.top-list-item {
  display: flex;
  padding-top: 20px; 
}

.top-img {
  width: 100px;
  height: 100px;
}

.top-img image {
  width: 100%;
  height: 100%;
  overflow: hidden;
}

.singer-list {
  flex: 1;
  display: flex;
  -ms-flex-direction: column;
  flex-direction: column;
  -ms-flex-pack: center;
  justify-content: center;
  padding: 0 20px;
  height: 100px;
  width: 480rpx;
  overflow: hidden;
  background: #333;
  color: hsla(0,0%,100%,.3);
  font-size: 12px;
}

.singer-list-item {
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  line-height: 26px;
}

.singer-list-title {
  color: #ffcd32;
  font-size: 16px;
}

/* search */


.search {
  position: relative;
  display: flex;
  flex-direction: column;
  padding: 0 40rpx;
  width: 100%;
}

.search-box-wrapper {
  margin: 20rpx 0;
}

.search-box {
  display: flex;
  -ms-flex-align: center;
  align-items: center;
  box-sizing: border-box;
  width: 100%;
  padding: 0 6px;
  height: 40px;
  background: #333;
  border-radius: 6px;
}

.input-box {
    flex: 1;
    margin: 0 5px;
    line-height: 18px;
    background: #333;
    color: #fff;
    font-size: 14px;
    outline: 0;
}

.shortcut-wrapper {
  position: absolute;
  flex: 1;
  top: 160rpx;
  right: 40rpx;
  left: 40rpx;
  bottom: 0;
  overflow: hidden;
  background-color: #222222;
}

.search-result-item {
  display: flex;
  align-items: center;
  padding-bottom: 40rpx;
}

.search-scroll {
  height: 100%;
}

.search-icon-music {
  margin-right: 20rpx;
  margin-left: 30rpx;
}

.result-text {
  flex: 1;
  font-size: 16px;
  color: hsla(0,0%,100%,.3);
  overflow: hidden;
}

.icon-close {
  margin: 3rpx;
  width: 24rpx;
  height: 24rpx;
  background-size: cover;
  background-image: url(data:image/svg+xml;base64,)
}

.icon-user {
  width: 30rpx;
  height: 30rpx;
  background-size: cover;
  background-image: url(data:image/png;base64,)
}

.icon-music {
  width: 30rpx;
  height: 30rpx;
  background-size: cover;
  background-image: url('data:image/svg+xml;base64,')
}

.search-result-item {

}

.shortcut-wrapper-item {
  margin-bottom: 40rpx;
}

.title {
  margin-bottom: 20px;
  font-size: 14px;
  color: hsla(0,0%,100%,.5);
}

.search-text {
    display: inline-block;
    padding: 5px 10px;
    margin: 0 20px 10px 0;
    border-radius: 6px;
    background: #333;
    font-size: 14px;
    color: hsla(0,0%,100%,.3);
}

.clear {
  display: flex;
  justify-content: space-between;
}

.icon-clear {
  width: 32rpx;
  height: 32rpx;
  background-size: cover;
  background-image: url('data:image/png;base64,');
}


.history-text {
  flex: 1;
  color: hsla(0,0%,100%,.5);
  font-size: 16px;
  display: flex;
  -ms-flex-align: center;
  align-items: center;
  height: 40px;
}

.history-item {
  display: flex;
  justify-content: space-between;
  align-items: center;
  overflow: hidden;
}

.icon-dismiss {
  width: 24rpx;
  height: 24rpx;
  background-size: cover;
  background-image: url('data:image/png;base64,');
}

.icon-search {
  width: 32rpx;
  height: 32rpx;
  background-size: cover;
  background-image: url('data:image/png;base64,')
}
