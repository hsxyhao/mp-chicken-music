.add-song {
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0;
  top: 0;
  display: flex;
  flex-direction: column;
}

.add-header {
  position: relative;
  height: 100rpx;
  text-align: center;
}

.header-text {
  line-height: 100rpx;
  font-size: 18px;
  color: #fff;
}

.search-box-wrapper {
  margin: 20px;
}

.search-box {
  display: flex;
  -ms-flex-align: center;
  align-items: center;
  box-sizing: border-box;
  width: 100%;
  padding: 0 6px;
  height: 40px;
  background: #333;
  border-radius: 6px;
}

.search-input {
  flex: 1;
  margin: 0 5px;
  line-height: 18px;
  background: #333;
  color: #fff;
  font-size: 14px;
  outline: 0;
}

.shortcut {
  display: flex;
  -ms-flex-align: center;
  align-items: center;
  width: 240px;
  margin: 0 auto;
  border: 1px solid #333;
  border-radius: 5px;
}

.shortcut-item {
  color: hsla(0,0%,100%,.3);
  flex: 1;
  padding: 16rpx;
  text-align: center;
  font-size: 14px;
}

.shortcut-item-selected {
  background: #333;
  color: #fff;
}

.result-list {
  flex: 1;
  padding: 20px 30px;
}

.result-list-scroll {
  height: 100%;
  background-color: rebeccapurple;
}

.icon-no {
  position: absolute;
  left: 0;
  top: 0;
  margin: 30rpx;
  width: 40rpx;
  height: 40rpx;
  color: #ffcd32;
  background-size: cover;
  background-image: url('data:image/png;base64,')
}

.icon-search {
  width: 68rpx;
  height: 68rpx;
  background-size: cover;
  background-image: url('data:image/png;base64,')
}