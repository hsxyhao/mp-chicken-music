.mini-audio {
  display: flex;
  -ms-flex-align: center;
  align-items: center;
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 180;
  width: 100%;
  height: 120rpx;
  background: #333;
  justify-content: space-between;
}

.mini-audio-left {
  display: flex;
  align-items: center;
}

.img-wrapper {
  width: 80rpx;
  height: 80rpx;
  margin-left: 40rpx;
}

.mini-audio-left .song-img {
  width: 100%;
  height: 100%;
  border-radius: 50%;
}

.song-info {
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  margin-left: 20rpx;
}

.song-info .song-name {
  margin-bottom: 2px;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  font-size: 14px;
  color: #fff;
}

.song-info .singer-name {
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  font-size: 12px;
  color: hsla(0,0%,100%,.3);
}

.mini-audio-right {
  display: flex;
  width: 200rpx;
  height: 100%;
  align-items: center;
  justify-content: space-around;
}

.icon-play-mini {
  width: 60rpx;
  height: 60rpx;
  background-size: cover;
  background-image: url('data:image/png;base64,')
}

.icon-pause-mini {
  width: 60rpx;
  height: 60rpx;
  background-size: cover;
  background-image: url('data:image/png;base64,')
}

