}

.icon-play {
  background-size: cover;
  background-image: url('data:image/png;base64,');
}

.play-mode-text {
  margin-left: 20rpx;
  font-size: 14px;
  color: hsla(0,0%,100%,.5);
}

.playlist-title .title-left {
  display: flex;
  align-items: center;
} 

.playlist-title {
  display: flex;
  justify-content: space-between;
  position: relative;
  align-items: center;
  padding:40rpx 50rpx 20rpx 40rpx;
}

.playlist-music {
  padding:0rpx 50rpx 20rpx 40rpx;
}

.playlist {
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  z-index: 200;
  background-color: rgba(0,0,0,.3);
}

.playlist-music .icon-mini {
  width: 24rpx;
  height: 24rpx;
  margin-right: 20rpx;
}

.playlist-music .text {
  flex: 1;
  letter-spacing: 4rpx;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  font-size: 14px;
  color: hsla(0,0%,100%,.3);
}

.playlist-music-item {
  display: flex;
  align-items: center;
  margin: 4px 0;
  height: 80rpx;
}

.wrapper-temp {
  position: relative;
  height: 100%;
  width: 100%;
}

.list-wrapper {
  position: absolute;
  left: 0;
  width: 100%;
  background-color: #333;
}

.playlist-add {
  display: flex;
  justify-content: center;
  margin-bottom: 60rpx;
}

.add-wrapper {
  display: flex;
  -ms-flex-align: center;
  align-items: center;
  justify-content: center;
  padding: 12rpx 30rpx;
  border: 1px solid hsla(0,0%,100%,.5);
  border-radius: 100px;
  color: hsla(0,0%,100%,.5);
}

.add-wrapper text {
  font-size: 12px;
  letter-spacing:2px;
}

.add-wrapper .icon-add {
  margin-right: 10rpx;
}

.playlist-close {
  text-align: center;
  line-height: 50px;
  background: #222;
  font-size: 16px;
  color: hsla(0,0%,100%,.5);
}


